    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amissemer/ywiki-plugins.git"
  },
  "author": "Adrien Missemer <adrien.missemer@sap.com>",
  "bugs": {
    "url": "https://github.com/amissemer/ywiki-plugins/issues"
  },
  "homepage": "https://github.com/amissemer/ywiki-plugins#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "ajv": "^6.5.0",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.8.0",
    "bootstrap-notify": "^3.1.3",
    "bootstrap-select": "^1.13.1",
    "bootstrap-validator": "^0.11.9",
    "chai-as-promised": "^7.1.1",
    "font-awesome": "^4.7.0",
    "imports-loader": "^0.7.1",
    "jquery": "^3.3.1",
    "jquery-contextmenu": "^2.7.0",
    "jquery-deferred": "^0.3.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1",
    "jsrender": "^0.9.90",
    "jsviews": "^0.9.90",
    "rxjs": "^6.3.1",
    "rxjs-compat": "^6.3.1",
    "underscore": "^1.9.0",
    "webpack-polyfills-plugin": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.2",
    "chai": "^4.1.2",
    "css-loader": "^0.27.3",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "file-loader": "^1.1.11",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.0",
