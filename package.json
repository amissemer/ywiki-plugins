{
  "name": "ywiki-plugins",
  "version": "1.0.0",
  "description": "The Golden Button wiki plugin",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --progress --watch",
    "test": "mocha-webpack --webpack-config webpack.test.config.js \"js/**/*.test.js\"",
    "prettier": "prettier --write js/**/*.js",
    "lint": "eslint --debug js/",
    "lint:write": "eslint js/ --fix"
  },
